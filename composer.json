{
  "name": "ghiyam/apix",
  "description": "Yii2 module supporting universal client-server requests to the external API",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "module",
    "API"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ghiya",
      "email": "g.mikadze@lakka.io"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "php-smpp/php-smpp": "^1.2"
  },
  "require-dev": {
    "yiisoft/yii2": "2.0.17"
  },
  "autoload": {
    "psr-4": {
      "ghiyam\\apix\\": "src/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
