{
  "name": "ghiyam/apix",
  "description": "Yii2 module supporting universal client-server requests to the external API.",
  "keywords": [
    "yii2",
    "module",
    "API"
  ],
  "type": "yii2-extension",
  "require": {
    "php": ">=5.5.0",
    "php-smpp/php-smpp": "^1.2"
  },
  "require-dev": {
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-debug": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "ghiyam\\apix\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ghiya",
      "email": "ghiya@mikadze.me"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
